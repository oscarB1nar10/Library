<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="NotesView">
        <attr name="title" format="string" />
        <attr name="charLimit" format="integer" />
    </declare-styleable>

    <declare-styleable name="CustomButton">
        <attr name="style" format="enum">
            <enum name="primary" value="0"/>
            <enum name="secondary" value="1"/>
            <enum name="tertiary" value="2"/>
            <enum name="positive" value="3"/>
            <enum name="negative" value="4"/>
            <enum name="cancel" value="5"/>
            <enum name="keyboard" value="6"/>
            <enum name="sticky" value="7"/>
        </attr>
        <attr name="enable" format="boolean"/>
    </declare-styleable>

</resources>